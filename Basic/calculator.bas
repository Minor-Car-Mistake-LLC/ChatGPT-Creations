10 INPUT "Enter first number: ", num1
20 INPUT "Enter second number: ", num2
30 INPUT "Enter operator (+, -, *, /): ", operator
40 IF operator = "+" THEN PRINT num1 + num2
50 IF operator = "-" THEN PRINT num1 - num2
60 IF operator = "*" THEN PRINT num1 * num2
70 IF operator = "/" THEN PRINT num1 / num2
80 END